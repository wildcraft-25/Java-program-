class OuterClass {
 void display() {
 System.out.println("OuterClass - Display method");
 Inner inner = new Inner();
 inner.display();
 }
 class Inner {
 void display() {
 System.out.println("Inner - Display method");
 }
 }
}
public class Main {
 public static void main(String[] args) {
 OuterClass outer = new OuterClass();
 outer.display();
 }
}
